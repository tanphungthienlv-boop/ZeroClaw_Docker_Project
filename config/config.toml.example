# ZeroClaw Configuration (TOML)
# Auto-generated by entrypoint.sh

default_provider = "custom:https://aiplatform.googleapis.com/v1/projects/chautinhtribot/locations/us-central1/endpoints/openapi"
default_model = "google/gemini-3-flash-preview"
default_temperature = 0.7

[memory]
backend = "sqlite"
auto_save = true

[gateway]
require_pairing = false
allow_public_bind = true

[channels_config]
cli = true

[channels_config.telegram]
bot_token = "YOUR_TELEGRAM_BOT_TOKEN_HERE"
allowed_users = ["*"]

# Fallback model chain
[[model_routes]]
hint = "flash3"
provider = "custom:https://aiplatform.googleapis.com/v1/projects/chautinhtribot/locations/us-central1/endpoints/openapi"
model = "google/gemini-3-flash-preview"

[[model_routes]]
hint = "flash2"
provider = "custom:https://aiplatform.googleapis.com/v1/projects/chautinhtribot/locations/us-central1/endpoints/openapi"
model = "google/gemini-2.0-flash"

[[model_routes]]
hint = "flash15"
provider = "custom:https://aiplatform.googleapis.com/v1/projects/chautinhtribot/locations/us-central1/endpoints/openapi"
model = "google/gemini-1.5-flash"

[secrets]
encrypt = false
